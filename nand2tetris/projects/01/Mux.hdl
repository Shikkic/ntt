/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand (a=sel, b=sel, out=nandss);
    Nand (a=a, b=nandss, out=nandas);
    Nand (a=sel, b=b, out=nandbs);
    Nand (a=nandas, b=nandbs, out=out);
}